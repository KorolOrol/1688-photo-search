{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "dist/popup.html",
      "default_icon": {
        "16": "dist/assets/icons/16.png",
        "48": "dist/assets/icons/48.png",
        "128": "dist/assets/icons/128.png"
      }
    },
    "icons": {
      "16": "dist/assets/icons/16.png",
      "48": "dist/assets/icons/48.png",
      "128": "dist/assets/icons/128.png"
    }
  }